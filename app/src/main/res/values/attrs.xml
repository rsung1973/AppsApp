<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ShownEdittext">
        <attr name="android:inputType"/>
        <attr name="android:hint"/>
        <attr name="android:textColorHint"/>
        <attr name="android:enabled"/>
        <attr name="android:imeOptions"/>
        <attr name="showButton" format="boolean"/>
        <attr name="nextFocusForward" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SettingsItemLayout">
        <attr name="layout_background" format="reference|integer" />
        <attr name="title_text" format="string" />
        <attr name="title_text_color" format="color" />
        <attr name="content_text" format="string" />
        <attr name="content_text_color" format="color" />
        <attr name="content_text_visible" format="boolean" />
        <attr name="value_hint_text" format="string" />
        <attr name="value_hint_text_color" format="color" />
        <attr name="switch_btn_drawable" format="reference|integer" />
        <attr name="switch_btn_visible" format="boolean" />
        <attr name="switch_checked" format="boolean" />
        <attr name="arrow_visible" format="boolean" />
        <attr name="progress_visible" format="boolean" />
        <attr name="clickable" format="boolean" />
        <attr name="android:entries" />
        <attr name="android:entryValues" />
    </declare-styleable>
</resources>